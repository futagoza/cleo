{
    "name": "cleo",
    "version": "0.0.0",
    "description": "Command line utilities for Node.js",
    "keywords": [
        "argv",
        "cli",
        "color",
        "config",
        "log",
        "option",
        "repl",
        "taskrunner"
    ],
    "repository": "futagoza/cleo",
    "license": "MIT",
    "author": "Futago-za Ryuu <futagoza.ryuu@gmail.com>",
    "files": [
        "out",
        "src"
    ],
    "main": "out/index.js",
    "dependencies": {
        "colourant": "1.0.0",
        "cosmiconfig": "5.2.1",
        "dateformat": "3.0.3",
        "tslib": "1.10.0"
    },
    "scripts": {
        "rebuild": "rimraf out && tsc -p src",
        "test": "nyc ava",
        "test-and-update-snapshots": "ava -u",
        "coverage": "codecov -f coverage/*.json",
        "lint": "eslint ."
    },
    "devDependencies": {
        "@types/dateformat": "3.0.0",
        "@futagoza/eslint-config-dev": "8.3.0",
        "@futagoza/eslint-config-typescript": "8.3.0",
        "@futagoza/tsconfig": "1.0.0",
        "ava": "2.2.0",
        "codecov": "3.5.0",
        "eslint": "6.1.0",
        "nyc": "14.1.1",
        "rimraf": "2.6.3",
        "ts-node": "8.3.0",
        "typescript": "3.5.3"
    },
    "engines": {
        "node": ">= 8"
    }
}
